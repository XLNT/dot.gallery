{
  "version": 2,
  "alias": [
    "dot.gallery"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    },
    {
      "src": "packages/backroom/src/server.ts",
      "use": "@now/node"
    },
    {
      "src": "packages/backroom/src/stripe.ts",
      "use": "@now/node"
    }
  ],
  "routes": [
    {
      "src": "/stripe/webhook",
      "dest": "packages/backroom/src/stripe_.ts"
    },
    {
      "src": "/graphql",
      "dest": "packages/backroom/src/server.ts"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "env": {
    "PRISMA_ENDPOINT": "https://gallery-v1-f5f0a401ff.herokuapp.com",
    "STRIPE_SECRET": "@backroom-stripe-secret"
  }
}
